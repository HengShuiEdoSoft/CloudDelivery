
.dui-up {
	-webkit-transition: all 0.3s;
	-o-transition: all 0.3s;
	transition: all 0.3s;
}
.dui-down {
	-webkit-transform: rotate(180deg);
	    -ms-transform: rotate(180deg);
	        transform: rotate(180deg);
	-webkit-transform-origin: center;
	    -ms-transform-origin: center;
	        transform-origin: center;
	-webkit-transition: all 0.3s;
	-o-transition: all 0.3s;
	transition: all 0.3s;
}
scroll-view {
	height: 90%;
}
.ui-divide-line {
	margin: 0 20rpx;
}
.dui-msgcenter-item {
	line-height: 60rpx;
}
.dui-drawer-title {
	width: 40%;
	font-size: 30rpx;
}
.dui-drawer-content {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	font-size: 30rpx;
	color: #797979;
}
.dui-drawer-content input {
	width: 40%;
	border: 1px solid #e2e2e2;
	border-radius: 10rpx;
	float: left;
	margin-right: 25rpx;
	font-size: 24rpx;
	text-align: center;
	padding: 10rpx 0;
}
.dui-drawer-hw-type {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.dui-hw-type-content {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	padding: 0 20rpx 20rpx 20rpx;
}
.dui-hw-type-content text {
	display: inline-block;
	width: 25%;
	margin: 10rpx;
	padding: 10rpx;
	overflow: hidden;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	text-align: center;
	font-size: 24rpx;
	line-height: 40rpx;
	background-color: #F8F8F8;
}
.dui-drawer-hw-type input {
	float: left;
	width: 50%;
	text-align: center;
	border: none;
}
.dui-checkbox-box {
	margin-right: 40rpx;
}
.dui-checkbox {
	-webkit-transform: scale(0.8);
	    -ms-transform: scale(0.8);
	        transform: scale(0.8);
}
.dui-drawer-btn-box {
	position: absolute;
	bottom: 0;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.dui-drawer-btn-l,
.dui-drawer-btn-r {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	line-height: 120rpx;
	color: #fff;
	text-align: center;
	font-size: 32rpx;
	border-radius: 0;
}
.dui-drawer-btn-l:after,
.dui-drawer-btn-r:after {
	content: '';
	border: none;
}
.dui-drawer-btn-l {
	background-color: #F0AD4E;
}
.dui-drawer-btn-r {
	background-color: #DD524D;
}

